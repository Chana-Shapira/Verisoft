		this.mammals=mammals;
		this.carnivorous=carnivorous;
		this.mood=mood;
	}
	
	public void sayHello() {
		
	}
	
	public void sayHello(int _____) {
		
	}

	public boolean isMammals() {
		return mammals;
	}

	public void setMammals(boolean mammals) {
		this.mammals = mammals;
	}

	public boolean isCarnivorous() {
		return carnivorous;
